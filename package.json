{
  "name": "dual-llm-chat",
  "version": "0.0.0",
  "description": "A dual-LLM split-brain architecture with real-time chat interface and Neo4j integration",
  "main": "src/app.coffee",
  "scripts": {
    "dev": "nodemon --exec coffee src/app.coffee",
    "compile": "coffee --compile --output lib/ src/",
    "start": "coffee src/app.coffee",
    "neo4j:setup": "coffee bin/seed-neo4j.coffee",
    "docker:neo4j": "docker run --name neo4j -p7474:7474 -p7687:7687 -d -e NEO4J_AUTH=neo4j/password neo4j:latest",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "llm",
    "dual-brain",
    "neo4j",
    "coffeescript",
    "chat",
    "ollama"
  ],
  "author": "Robert de Forest <robert@defore.st",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "coffeescript": "^2.7.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "neo4j-driver": "^5.13.0",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "nodemon": "^3.0.1",
    "vite-plugin-coffee": "^2.2.1",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=24.2.0"
  }
}
